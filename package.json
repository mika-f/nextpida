{
  "private": true,
  "name": "nextpida",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "author": "Natsuneko",
  "main": "index.js",
  "scripts": {
    "changeset": "changeset",
    "check": "run-p check:*:*",
    "check:ts:pkg-nextpida": "tsc --noEmit --project ./packages/nextpida/tsconfig.json",
    "check:ts:pkg-nextpida-handler-types": "tsc --noEmit --project ./packages/nextpida-handler-types/tsconfig.json",
    "check:ts:pkg-nextpida-method-handler": "tsc --noEmit --project ./packages/nextpida-method-handler/tsconfig.json",
    "check:pkg": "prettier-package-json --list-different ./packages/**/package.json",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./packages/**/*.ts",
    "lint:prettier": "prettier --check --ignore-unknown ./packages/**/*.*",
    "fix": "run-s fix:*",
    "fix:eslint:": "eslint --fix ./packages/**/*.ts",
    "fix:prettier": "prettier --write --ignore-unknown ./packages/**/*.*",
    "fix:pkg": "prettier-package-json --write ./packages/**/package.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-package-json": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.1",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.4",
    "typescript-transform-extensions": "^1.0.1"
  },
  "keywords": []
}
